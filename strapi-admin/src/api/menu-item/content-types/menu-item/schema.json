{
  "kind": "collectionType",
  "collectionName": "menu_items",
  "info": {
    "singularName": "menu-item",
    "pluralName": "menu-items",
    "displayName": "Menu Item",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Photo": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "Price": {
      "type": "integer",
      "required": true
    },
    "Type": {
      "type": "enumeration",
      "enum": [
        "Bubble Tea",
        "Pinoy Dishes",
        "Pinoy Snacks",
        "Pastries / Deserts",
        "Frozen Items",
        "Other"
      ],
      "required": true
    },
    "Hide": {
      "type": "boolean",
      "default": false,
      "required": true
    }
  }
}
